<% #button %>
<%-- ATTN: onMouseUp event is needed for some scripts, for the event object! --%>
<span><a href="#" onclick="return false" onmouseup="<%= param.onClick encoding="all" %>" class="edit-button<%= param.className prefix=" " %>" id="<%= param.name %>"><%= param.value %></a></span><%-%>

<% #buttons %>
<div class="edit-buttons-left">
<% if (param.spacer) %>
<div class="edit-spacer"></div>
<% end %>
<%= param.buttons %>
</div>
