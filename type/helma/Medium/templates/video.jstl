<video<%-%>
<%= param.id prefix=' id="' suffix='"' -%>
<%= param.src prefix=' src="' suffix='"' -%>
<%= param.poster prefix=' poster="' suffix='"' -%>
<%= param.width prefix=' width="' suffix='"' -%>
<%= param.height prefix=' height="' suffix='"' -%>
<%= ' controller' if=param.controller -%>
<%= ' autoplay' if=param.autoplay -%>
<%= ' loop' if=param.loop -%>
<%= ' preload' if=param.preload -%>
<%= ' autobuffer' if=param.autobuffer -%>></video>
