<div class="<%= param.iconSmall ? "icon-small" : "icon" %>">
<% this.link $icon %><%= param.details %>
</div>

<% $icon %>
<% if (param.iconCentered) %>
<div><img src="<%= param.src %>" width="<%= param.width %>" height="<%= param.height %>"></div>
<span><%= param.name %></span><%-%>
<% else %>
<img src="<%= param.src %>" width="<%= param.width %>" height="<%= param.height %>"><span><%= param.name %></span><%-%>
<% end %>
