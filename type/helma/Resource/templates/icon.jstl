<span class="<%= param.small ? "icon-small" : "icon" %>">
<%= this.renderLink($icon) %><%= param.details %>
</span>

<% $icon %>
<% if (param.small) %>
<img src="<%= param.src %>" width="16" height="16">&nbsp;<%= param.name %>
<% else %>
<div><img src="<%= param.src %>" width="32" height="32"></div>
<span><%= param.name %></span>
<% end %>
