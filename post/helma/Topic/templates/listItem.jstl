		<td width="180"><%= this.renderLink(encode(this.getTitle().breakAt(30))) %></td>
		<td><%= this.renderUser() %></td>
		<td nowrap>
		<% $count = this.count() - 1 %>
		<% if ($count == 1) %>
			1 Post
		<% elseif ($count > 1) %>
			<%= $count %> Posts
		<% else %>
			&nbsp;
		<% end %>
		</td>
		<td nowrap><%= app.data.dateMedium.format(this.modificationDate) %></td>
